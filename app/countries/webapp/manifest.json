{
    "_version": "1.32.0",
    "sap.cloud": {
        "public": true,
        "service": "triporder-ui" 
    },
    "sap.app": {
        "id": "countries",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "dataSources": {
            "mainService": {
                "uri": "/draft/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        },
        "offline": false,
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "ui5template.fiorielements.v4.lrop",
            "version": "1.0.0"
        },
        "crossNavigation": {
            "inbounds": {
                "countries-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "countries_configuration",
                    "action": "Manage Countries",
                    "title": "{{flpTitle}}",
                    "subTitle": "{{flpSubtitle}}",
                    "info": "{{flpInfo}}",
                    "icon": ""
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "resources": {
            "js": [],
            "css": []
        },
        "dependencies": {
            "minUI5Version": "1.96.0",
            "libs": {
                "sap.ui.core": {},
                "sap.fe.templates": {}
            }
        },
        "models": {
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            }
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "CountriesList",
                    "target": "CountriesList"
                },
                {
                    "pattern": "Countries({key}):?query:",
                    "name": "CountriesObjectPage",
                    "target": "CountriesObjectPage"
                },
                {
                    "pattern": "Countries({key})/texts({key2}):?query:",
                    "name": "Countries_textsObjectPage",
                    "target": "Countries_textsObjectPage"
                }
            ],
            "targets": {
                "CountriesList": {
                    "type": "Component",
                    "id": "CountriesList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "Countries",
                            "variantManagement": "Page",
                            "navigation": {
                                "Countries": {
                                    "detail": {
                                        "route": "CountriesObjectPage"
                                    }
                                }
                            }
                        }
                    }
                },
                "CountriesObjectPage": {
                    "type": "Component",
                    "id": "CountriesObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "editableHeaderContent": false,
                            "entitySet": "Countries",
                            "navigation": {
                                "texts": {
                                    "detail": {
                                        "route": "Countries_textsObjectPage"
                                    }
                                }
                            }
                        }
                    }
                },
                "Countries_textsObjectPage": {
                    "type": "Component",
                    "id": "Countries_textsObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "editableHeaderContent": false,
                            "entitySet": "Countries_texts"
                        }
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.platform.abap": {
        "_version": "1.1.0",
        "uri": ""
    },
    "sap.platform.hcp": {
        "_version": "1.1.0",
        "uri": ""
    },
    "sap.fiori": {
        "_version": "1.1.0",
        "registrationIds": [],
        "archeType": "transactional"
    }
}